[
  { "name": "basicArithmetic" },
  { "left": "a+b", "right": "c", "vars": ["a", "b", "c"], "swap": "true" },
  { "left": "a-b", "right": "c", "vars": ["a", "b", "c"], "swap": "true" },
  {
    "left": "a\\cdot b",
    "right": "c",
    "vars": ["a", "b", "c"],
    "swap": "true"
  },
  {
    "left": "\\frac{a}{b}",
    "right": "c",
    "vars": ["a", "b", "c"],
    "swap": "true"
  },
  { "name": "interchange" },
  { "left": "a+b", "right": "b+a", "vars": ["a", "b"] },
  { "left": "a\\cdot b", "right": "b\\cdot a", "vars": ["a", "b"] },
  { "name": "set/removeBrackets" },
  { "name": "association" },
  { "left": "\\left(a\\right)", "right": "a", "vars": ["a"], "swap": "true" },
  {
    "left": "a+\\left(b+c\\right)",
    "right": "a+b+c",
    "vars": ["a", "b", "c"],
    "swap": "true",
    "name": "plusBracket"
  },
  {
    "left": "a+\\left(b-c\\right)",
    "right": "a+b-c",
    "vars": ["a", "b", "c"],
    "swap": "true",
    "name": "plusBracket"
  },
  {
    "left": "a-\\left(b+c\\right)",
    "right": "a-b-c",
    "vars": ["a", "b", "c"],
    "swap": "true",
    "name": "minusBracket"
  },
  {
    "left": "a-\\left(b-c\\right)",
    "right": "a-b+c",
    "vars": ["a", "b", "c"],
    "swap": "true",
    "name": "minusBracket"
  },
  { "name": "distribution" },
  {
    "left": "\\left(a+b\\right)\\cdot\\left(c+d\\right)",
    "right": "a\\cdot c+a\\cdot d+b\\cdot c+b\\cdot d",
    "vars": ["a", "b", "c", "d"],
    "name": "multiplyOut"
  },
  {
    "left": "\\left(a-b\\right)\\cdot\\left(c-d\\right)",
    "right": "a\\cdot c-a\\cdot d-b\\cdot c+b\\cdot d",
    "vars": ["a", "b", "c", "d"],
    "name": "multiplyOut"
  },
  {
    "left": "a\\cdot\\left(b+c\\right)",
    "right": "a\\cdot b+a\\cdot c",
    "vars": ["a", "b", "c"],
    "name": "multiplyOut"
  },
  {
    "left": "a\\cdot\\left(b-c\\right)",
    "right": "a\\cdot b-a\\cdot c",
    "vars": ["a", "b", "c"],
    "name": "multiplyOut"
  },
  {
    "left": "a\\cdot b+a\\cdot c",
    "right": "a\\cdot\\left(b+c\\right)",
    "vars": ["a", "b", "c"],
    "name": "outsideBrackets"
  },
  {
    "left": "a\\cdot b-a\\cdot c",
    "right": "a\\cdot\\left(b-c\\right)",
    "vars": ["a", "b", "c"],
    "name": "outsideBrackets"
  },
  { "name": "negativeSign" },
  { "left": "-a", "right": "\\left(-a\\right)", "vars": ["a"] },
  { "left": "-\\left(-a\\right)", "right": "a", "swap": "true", "vars": ["a"] },
  {
    "left": "a+\\left(-b\\right)",
    "right": "a-b",
    "swap": "true",
    "vars": ["a", "b"]
  },
  {
    "left": "a-\\left(-b\\right)",
    "right": "a+b",
    "swap": "true",
    "vars": ["a", "b"]
  },
  {
    "left": "\\left(-a\\right)\\cdot\\left(-b\\right)",
    "right": "a\\cdot b",
    "swap": "true",
    "vars": ["a", "b"]
  },
  {
    "left": "\\left(-a\\right)\\cdot b",
    "right": "\\left(-a\\cdot b\\right)",
    "swap": "true",
    "vars": ["a", "b"]
  },
  {
    "left": "a\\cdot\\left(-b\\right)",
    "right": "\\left(-a\\cdot b\\right)",
    "swap": "true",
    "vars": ["a", "b"]
  },
  {
    "swap": "true",
    "left": "\\frac{\\left(-a\\right)}{b}",
    "right": "\\left(-\\frac{a}{b}\\right)",
    "vars": ["a", "b"]
  },
  {
    "swap": "true",
    "left": "\\frac{a}{\\left(-b\\right)}",
    "right": "\\left(-\\frac{a}{b}\\right)",
    "vars": ["a", "b"]
  },
  { "name": "binomialFormula" },
  {
    "left": "(a+b)^2",
    "right": "a^2+2\\cdot a\\cdot b+b^2",
    "vars": ["a", "b"],
    "swap": "true"
  },
  {
    "left": "(a-b)^2",
    "right": "a^2-2\\cdot a\\cdot b+b^2",
    "vars": ["a", "b"],
    "swap": "true"
  },
  {
    "left": "(a^2-b^2)",
    "right": "(a-b)\\cdot(a+b)",
    "vars": ["a", "b"],
    "swap": "true"
  },
  { "name": "quadraticEquation" },
  {
    "left": "a\\cdot x^2+b\\cdot x+c=0",
    "right": "x_{1,2}=\\frac{-b\\pm\\sqrt{b^2-4\\cdot a\\cdot c}}{2\\cdot a}",
    "vars": ["a", "b", "c"],
    "name": "abcRule"
  },
  {
    "left": "x^2+p\\cdot x+q=0",
    "right": "x_{1,2}=\\frac{-p}{2}\\pm\\sqrt{(\\frac{p}{2})^2-q}",
    "vars": ["p", "q"],
    "name": "pqRule"
  },
  { "name": "fraction" },
  {
    "left": "\\frac{a}{c}+\\frac{b}{c}",
    "right": "\\frac{a+b}{c}",
    "vars": ["a", "b", "c"]
  },
  { "name": "power" },
  { "left": "(a^n)^m", "right": "a^{n\\cdot m}", "vars": ["a", "n", "m"] }
]
